@model laohaldussüsteemTsausova.Models.Product

@{
    ViewBag.Title = "Menu";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Футболисты</title>
</head>
<body>
    <div>
        <h3>Список игроков</h3>
        <table>
            <tr>
                <td><p>Имя</p></td>
                <td><p>Возраст</p></td>
                <td><p>Позиция</p></td>
                <td></td>
            </tr>
            @foreach (var p in ViewBag.Products)
            {
                <tr>
                    <td><p>@p.Toode</p></td>
                   
                </tr>
            }
        </table>
    </div>
</body>
</html>
